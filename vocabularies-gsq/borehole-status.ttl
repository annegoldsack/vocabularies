    skos:inScheme cs: ;
    skos:broader :injecting ;
.

:injecting-water
    a skos:Concept ;
  dcterms:references "Water injection (oil production). Wikipedia, accessed by WA, April 2023 <https://en.wikipedia.org/wiki/Water_injection_(oil_production)>"@en ;
    skos:prefLabel "Injecting Water"@en ;
    skos:definition "Waterflooding or water injection is where water is injected into the oil reservoir, to maintain pressure (also known as voidage replacement) or to drive oil towards the wells, and thereby increase production. Water-injection wells are common offshore, where filtered and treated seawater is injected into a lower water-bearing section of the reservoir. "@en ;
    skos:historyNote "From WA [GSWA Borehole Status, quoting Wikipedia for ‘Water injection (oil production)’; April 2023]." ;
    skos:scopeNote "Used by WA (for ‘water injection’)." ;
    skos:altLabel "Water Injection"^^:WA ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :injecting ;
.

