    skos:hasTopConcept
        scm:cadastre ,
        scm:estimate ,
        scm:gps ,
        scm:imagery-automated ,
        scm:imagery-manual ,
        scm:map-digitised ,
        scm:non-spatial ,
        scm:other ,
        scm:survey-data ,
        scm:unknown ;
    skos:historyNote "This vocabulary was created by the Queensland Spatial Information services." ;
    skos:prefLabel "Spatial capture method"@en ;
    prov:qualifiedAttribution [
            dcat:hadRole isoroles:custodian ;
            prov:agent [
                    a prov:Agent ;
                    rdfs:label "Icsm"
                ]
        ] ;
