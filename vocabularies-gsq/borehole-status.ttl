    skos:definition "Coal seam gas well that has been permanently sealed."@en ;
    dcterms:provenance "Sourced from NSW." ;
    skos:scopeNote "Used by NSW." ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :abandoned ;
.

