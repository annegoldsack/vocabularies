PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX isoroles: <http://def.isotc211.org/iso19115/-1/2018/CitationAndResponsiblePartyInformation/code/CI_RoleCode/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rs: <https://linked.data.gov.au/def/road-seasonality>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

rs:no-seasonal-closure
    a skos:Concept ;
    skos:altLabel
        "F"@en ,
        "False"@en ;
    skos:definition "False - no seasonal restrictions."@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme rs: ;
    skos:prefLabel "No Seasonal Closure"@en ;
.

rs:seasonal-closure
    a skos:Concept ;
    skos:altLabel
        "T"@en ,
        "True"@en ;
    skos:definition "True - subject to seasonal closure."@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme rs: ;
    skos:prefLabel "Seasonal Closure"@en ;
.

rs:unknown
    a skos:Concept ;
    skos:altLabel "U"@en ;
    skos:definition "Unknown â€“ it is unknown if this road is affected by seasonal closures."@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme rs: ;
    skos:prefLabel "Unknown"@en ;
.

rs:
    a skos:ConceptScheme ;
    dcterms:created "2023-05-30"^^xsd:date ;
    dcterms:creator [
            a prov:Agent ;
            rdfs:label "Department Of Resources"
        ] ;
    dcterms:identifier "road-seasonality"^^xsd:token ;
    dcterms:modified "2023-05-30"^^xsd:date ;
    dcterms:publisher [
            a prov:Agent ;
            rdfs:label "Icsm"
        ] ;
    skos:definition "This vocabulary describes whether a road may be subjected to seasonal road closures."@en ;
    skos:hasTopConcept
        rs:no-seasonal-closure ,
        rs:seasonal-closure ,
        rs:unknown ;
    skos:historyNote "This vocabulary was created by the Queensland Spatial Information services." ;
    skos:prefLabel "Road Seasonality"@en ;
    prov:qualifiedAttribution [
            dcat:hadRole isoroles:custodian ;
            prov:agent [
                    a prov:Agent ;
                    rdfs:label "Icsm"
                ]
        ] ;
.

