rm:
    a skos:ConceptScheme ;
    dcterms:created "2023-05-31"^^xsd:date ;
    dcterms:creator [
            a prov:Agent ;
            rdfs:label "Department Of Resources"
        ] ;
    dcterms:identifier "road-maintainer"^^xsd:token ;
    dcterms:modified "2023-05-31"^^xsd:date ;
    dcterms:publisher [
            a prov:Agent ;
            rdfs:label "Icsm"
        ] ;
    skos:definition "This vocabulary describes the maintainer of a road."@en ;
    skos:hasTopConcept
        rm:local-government ,
        rm:other ,
        rm:port-authority ,
        rm:private ,
        rm:qpws ,
        rm:state ,
        rm:tmr ,
        rm:unknown ,
        rm:unmaintained ,
        rm:utility-provider ,
        rm:water-authority ;
    skos:historyNote "This vocabulary was created by the Queensland Spatial Information services." ;
    skos:prefLabel "Road Maintainer"@en ;
    prov:qualifiedAttribution [
            dcat:hadRole isoroles:custodian ;
            prov:agent [
                    a prov:Agent ;
                    rdfs:label "Icsm"
                ]
        ] ;
.

