PREFIX : <http://linked.data.gov.au/def/borehole-status/>
PREFIX agldwgstatus: <https://linked.data.gov.au/def/reg-statuses/>
PREFIX cs: <http://linked.data.gov.au/def/borehole-status>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX reg: <http://purl.org/linked-data/registry#>
PREFIX sdo: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:proposed
    a skos:Concept ;
    skos:prefLabel "Proposed"@en ;
    skos:definition "A concept or application exists to drill a borehole, but drilling has not yet commenced or been approved"@en ;
    skos:historyNote "Modified from: Geoscience Australia, modified from PPDM Wellbore Status vocabulary, 2014; CoalLog. Similar to PPDM's 'location' classification" ;
    skos:usageNote "Used by GA, QLD, BOM" ;
    skos:altLabel "New Well"^^:NT ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :not-operational ;
.

:application
    a skos:Concept ;
    skos:prefLabel "Application"@en ;
    skos:definition "A borehole that is intended and has had an application for activity submitted to the governing authority that is pending assessment and outcome"@en ;
    skos:historyNote "From GSQ" ;
    skos:usageNote "Only used in QLD" ;
    skos:altLabel "Planned"^^:NOPSEMA ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :proposed ;
.

:approved
    a skos:Concept ;
    skos:prefLabel "Approved"@en ;
    skos:definition "An application for a proposed borehole has been approved, but drilling has not yet commenced"@en ;
    skos:historyNote "Derived from QLD's term" ;
    skos:usageNote "Only used in QLD" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :proposed ;
.

:not-drilled
    a skos:Concept ;
    skos:prefLabel "Not Drilled"@en ;
    skos:definition "A borehole that was proposed in concept or approved on application, but was never drilled. This does not include the circumstance where the status is unknown"@en ;
    skos:historyNote "Created for the Borehole Status vocabulary harmonisation initiative in 2023" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :proposed ;
.

:not-approved
    a skos:Concept ;
    skos:prefLabel "Not Approved"@en ;
    skos:definition "A application was submitted to drill a borehole, but was not approved, encompassing outright dismissal or requiring amendment"@en ;
    skos:historyNote "Created for the Borehole Status vocabulary harmonisation initiative in 2023" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :proposed ;
.

:in-progress
    a skos:Concept ;
    skos:prefLabel "In Progress"@en ;
    skos:definition "The status of a borehole commencing from the rig being moved onsite to the time that drilling operations are completed" ;
    skos:historyNote "From GA. Similar to PPDM's 'drilling'" ;
    skos:usageNote "Used by GA, NOPSEMA" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :not-operational ;
.

:drilling-suspended
    a skos:Concept ;
    skos:prefLabel "Not Approved"@en ;
    skos:definition "Drilling operations have been temporarily suspended (eg, for weather, maintenance, or other reasons)."@en ;
    skos:historyNote "Created for the Borehole Status vocabulary harmonisation initiative in 2023" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :in-progress ;
.

:drilling-completed
    a skos:Concept ;
    skos:prefLabel "Drilling Completed"@en ;
    skos:definition "Drilling is completed. Production infrastructure may be installed. The borehole has not necessarily been abandoned.  Note that the CoalLog 'completed' status is more similar to 'abandoned' in this vocabulary" ;
    skos:historyNote "Modified from GA, for: completed" ;
    skos:altLabel "Drilling"^^:WA ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :not-operational ;
.

:operational
    a skos:Concept ;
    skos:prefLabel "Operational"@en ;
    skos:definition "Borehole is producing, injecting or monitoring (investigating, observational)" ;
    skos:historyNote "Modified from NOPSEMA, to include monitoring" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:topConceptOf cs: ;
.

:producing
    a skos:Concept ;
    skos:prefLabel "Producing"@en ;
    skos:definition "The status of a borehole during which liquids or gas are being extracted from the borehole"@en ;
    skos:historyNote "Sourced from GA" ;
    skos:usageNote "Used by GA, QLD, NZ, WA" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:altLabel "On Production"^^:QLD , "Producer"^^:VIC ;
    skos:broader :operational ;
.

:operational-not-producing
    a skos:Concept ;
    skos:prefLabel "Operational Not Producing"@en ;
    skos:definition "A site that is an active part of a project or operation that typically contributes to production, but is not currently due to other activities, such as a well undergoing routine maintenance that is not suspended, or a Mining Lease in which excavation is occuring after construction, but prior to, or between, extraction phases"@en ;
    skos:historyNote "From QLD" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :operational ;
.

:producing-gas
    a skos:Concept ;
    skos:prefLabel "Producing Gas"@en ;
    skos:definition "Coal seam gas wells producing gas in NSW"@en ;
    skos:historyNote "Used in NSW" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :producing ;
.

:injecting
    a skos:Concept ;
    skos:prefLabel "Injecting"@en ;
    skos:definition "The status of a borehole during which liquids or gas are being pumped into the borehole"@en ;
    skos:historyNote "From GA" ;
    skos:usageNote "Used by GA, NZ" ;
    skos:altLabel "Injector"^^:NT , "Injection"^^:WA , "INJ"^^:VIC  ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :operational ;
.

:reinjecting
    a skos:Concept ;
    skos:prefLabel "Re-injecting"@en ;
    skos:definition "None given"@en ;
    skos:historyNote "From NT" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :operational ;
.

:producing-and-injecting
    a skos:Concept ;
    skos:prefLabel "Producing and Injecting"@en ;
    skos:definition "The status of a borehole that alternately produces and injects liquid or gas into a borehole."@en ;
    skos:historyNote "From GA" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :producing , :injecting ;
.

:monitoring
    a skos:Concept ;
    skos:prefLabel "Monitoring"@en ;
    skos:definition "A borehole/well that is actively monitoring reservoir, aquifer or other geological parameters"@en ;
    skos:historyNote "From QLD" ;
    skos:usageNote "Used by GA, QLD, NT & SA" ;
    skos:altLabel "Observation"^^:NT , "UCG"^^:SA , "Investigation"^^:SA , "UCG"^^:SA  , "Observation"^^:VIC ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :operational ;
.

:water-supply
    a skos:Concept ;
    skos:prefLabel "Water Supply"@en ;
    skos:definition "A borehole/well that is primarily producing water"@en ;
    skos:historyNote "From QLD" ;
    skos:altLabel "Water Well"^^:SA ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :producing ;
.

:reconditioned
    a skos:Concept ;
    skos:prefLabel "Reconditioned"@en ;
    skos:definition "Borehole (e.g., groundwater feature) has been reconditioned so that functionality has improved"@en ;
    skos:historyNote "Modified from BOM" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :operational ;
.

:not-operational
    a skos:Concept ;
    skos:prefLabel "Not Operational"@en ;
    skos:definition "A well that is not operating nor permanently abandoned. Such a well is either shut-in or suspended. A well that is no longer operating but is not fully decommissioned is in one of 3 statuses: Shut-in, Plugged, Suspended. Note (NOPSEMA): Would prefer to avoid using the term 'suspended' as it has a variety of definitions, but the term is intended to reflect the wording in the regulations. For well integrity monitoring purposes, the distinction between shut in and suspended is critical." ;
    skos:historyNote "From NOPSEMA. NOPSEMA prefers to avoid using the term 'suspended' as it has a variety of definitions, but the term is intended to reflect the wording in the regulations. For well integrity monitoring purposes, the distinction between shut in and suspended is critical." ;
    skos:usageNote "Not operational shut in, Not operational plugged, Not operational suspended (latter two are equivalent, suggested by NOPSEMA notes for 'Not operational suspended'). Equivalent to 'plugged' defined in Section 10.11.2 of OGUK Well Lifecycle Integrity Guidelines Issue 4." ;
    skos:altLabel "Inactive"^^:QLD ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:topConceptOf cs: ;
.

:suspended
    a skos:Concept ;
    skos:prefLabel "Suspended"@en ;
    skos:definition "Borehole operations have been halted for a significant period of time or in a manner that requires significant effort for the resumption of activities. The borehole can be plugged downhole and fluids injected on top of the plug in order to create a temporary seal to prevent hydrocarbon leakage. The surface equipment remains in place. Suspended is never the final status of a borehole."@en ;
    skos:historyNote "From GA" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :not-operational ;
.

:shut-in
    a skos:Concept ;
    skos:prefLabel "Shut In"@en ;
    skos:definition "A borehole/well with one or more valves temporarily closed in the direction of flow"@en ;
    skos:historyNote "Mofified from NOPSEMA" ;
    skos:usageNote "Used by NZ, VIC," ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :not-operational ;
.

:cased-and-suspended
    a skos:Concept ;
    skos:prefLabel "Cased and Suspended"@en ;
    skos:definition "A well in which casing has been installed, but is suspended and inactive pending installation of completion, monitoring or other equipment"@en ;
    skos:historyNote "From QLD" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :not-operational ;
.

:abandoned
    a skos:Concept ;
    skos:prefLabel "Abandoned"@en ;
    skos:definition "All operations and activities are terminated. The operator has left the site with no intention of returning to the borehole. The borehole is not necessarily plugged." ;
    skos:historyNote "From GA" ;
    skos:altLabel "Decommissioned"^^:BOM , "Concluded"^^:QLD , "Completed" ;
    skos:usageNote "Used by GA & WA. Similar to CoalLog status 'completed'" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :not-operational ;
.

:plugged
    a skos:Concept ;
    skos:prefLabel "Plugged"@en ;
    skos:definition "The borehole has been filled or sealed. The extent of seal may vary from just plugged near the surface (eg, many coal boreholes) to plugged in order to prevent the migration of oil, gas, water, or other substance from one stratum to another (ie, PPDM definition)"@en ;
    skos:historyNote "Geoscience Australia, modified from PPDM Wellbore Status vocabulary, 2014" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :abandoned ;
.

:backfilled
    a skos:Concept ;
    skos:prefLabel "Backfilled"@en ;
    skos:definition "Borehole has been backfilled from the surface."@en ;
    skos:historyNote "From BOM, CoalLog" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :abandoned ;
.

:technically-abandoned
    a skos:Concept ;
    skos:prefLabel "Technically Abandoned"@en ;
    skos:definition "A borehole/well in which all penetrated zones with flow potential have been permanently isolated from surface and from lower pressured zones. Wellhead/conductor have been removed unless approved to leave in situ. Well has not yet been accepted by the regulatory authority (e.g., NOPSEMA) as abandoned"@en ;
    skos:historyNote "After NOPSEMA" ;
    skos:altLabel "Decommissioning" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :abandoned ;
.

:plugged-and-abandoned
    a skos:Concept ;
    skos:prefLabel "Plugged and Abandoned"@en ;
    skos:definition "An abandoned borehole that has also been plugged. The extent of seal may vary from just plugged near the surface (eg, many coal boreholes) to plugged to prevent the migration of oil, gas, water, or other substance from one stratum to another (ie, PPDM definition). The borehole/well has been accepted by the regulatory authority as being abandoned"@en ;
    skos:historyNote "Modified from: Geoscience Australia, modified from PPDM Wellbore Status vocabulary, 2014" ;
    skos:usageNote "GA definition also used by WA" ;
    skos:altLabel "Permanently Abandoned" , "Permanently Sealed" , "Plugged and Permanently Abandoned" ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:broader :abandoned , :plugged ;
.

:unknown
    a skos:Concept ;
    skos:prefLabel "Unknown"@en ;
    skos:definition "No clear documentation of status" ;
    skos:historyNote "NZ, after: PPDM Well Status and Classification, 2016" ;
    skos:usageNote "WA uses same; GA & QLD use similar" ;
    skos:altLabel "Not Recorded"^^:NT ;
    rdfs:isDefinedBy cs: ;
    skos:inScheme cs: ;
    skos:topConceptOf cs: ;
.


cs:
    a
        skos:ConceptScheme ;
    dcterms:created "2023-04-26"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/gsq> ;
    dcterms:modified "2023-05-11"^^xsd:date ;
    skos:historyNote "Compiled by the Geological Survey of Queensland for the Government Geoscience Information Committee (GGIC) in a Borehole information harmonisation initiative in 2023" ;
    dcterms:publisher <https://linked.data.gov.au/org/gsq> ;
    prov:qualifiedDerivation [
        prov:entity <http://linked.data.gov.au/def/site-status> ;
        prov:hadRole <http://linked.data.gov.au/def/vocab-derivation-modes/subsetting-and-extension> ;
    ] ;
    reg:status agldwgstatus:experimental ;
    skos:definition "The operational situation of a Borehole, also known as a Drillhole or a Wellbore. These statuses also apply to Bores or Wells that may be made of multiple Boreholes or Wellbores."@en ;
    skos:hasTopConcept
        :operational ,
        :not-operational ,
        :unknown ;
    skos:prefLabel "Borehole Status"@en ;
.

