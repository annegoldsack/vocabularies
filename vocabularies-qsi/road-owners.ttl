ro:
    a skos:ConceptScheme ;
    dcterms:created "2023-05-31"^^xsd:date ;
    dcterms:creator [
            a prov:Agent ;
            rdfs:label "Department Of Resources"
        ] ;
    dcterms:identifier "road-owner"^^xsd:token ;
    dcterms:modified "2023-05-31"^^xsd:date ;
    dcterms:publisher [
            a prov:Agent ;
            rdfs:label "Icsm"
        ] ;
    skos:definition "This vocabulary describes the owner of a road."@en ;
    skos:hasTopConcept
        ro:local-government ,
        ro:other ,
        ro:port-authority ,
        ro:private ,
        ro:qpws ,
        ro:state ,
        ro:tmr ,
        ro:unknown ,
        ro:utility-provider ,
        ro:water-authority ;
    skos:historyNote "This vocabulary was created by the Queensland Spatial Information services." ;
    skos:prefLabel "Road Owner"@en ;
    prov:qualifiedAttribution [
            dcat:hadRole isoroles:custodian ;
            prov:agent [
                    a prov:Agent ;
                    rdfs:label "Icsm"
                ]
        ] ;
.

