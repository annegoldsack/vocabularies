cs:
    a
        owl:Ontology ,
        skos:ConceptScheme ;
    dcterms:created "2019-09-25T13:23:29"^^xsd:dateTime ;
    dcterms:creator <https://linked.data.gov.au/org/gsq> ;
    dcterms:modified "2023-03-16"^^xsd:date ;
    dcterms:provenance "Defined by GSQ" ;
    dcterms:publisher <https://linked.data.gov.au/org/gsq> ;
    reg:status agldwgstatus:stable ;
    skos:definition "The number of measured parameters that describe the Fundamental Dataset generated by conducting a seismic survey. The Fundamental Dataset can be generalised as the resulting data output for a seismic survey: a 2D Line or collection of 2D Lines, a 3D Volume, or a 4D Temporal Volume."@en ;
    skos:hasTopConcept
        sdim:2d-seismic ,
        sdim:3d-seismic ,
        sdim:4d-seismic ;
    skos:prefLabel "Seismic Dimensionality"@en ;
.

